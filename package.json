{
  "name": "2md",
  "version": "0.0.0",
  "type": "module",
  "repository": "github:o-az/2md",
  "bin": {
    "2md": "./src/cli.mjs"
  },
  "files": [
    "src/cli.mjs"
  ],
  "imports": {
    "#*": "./src/*",
    "#package.json": "./package.json",
    "#wrangler.json": "./wrangler.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "NODE_ENV='production' wrangler --config='wrangler.json' deploy",
    "types": "wrangler types --env-interface CloudflareBindings",
    "check": "biome check . --write --unsafe",
    "check:types": "bun tsgo --project tsconfig.json --noEmit",
    "test:e2e": "./scripts/checks.sh",
    "prepublishOnly": "bun run build:cli"
  },
  "dependencies": {
    "hono": "^4.11.7",
    "pretty-cache-header": "^1.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@cloudflare/vite-plugin": "^1.22.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.3.7",
    "@types/node": "^25.1.0",
    "@typescript/native-preview": "^7.0.0-dev.20260128.1",
    "typescript": "^5.9.3",
    "vite": "^8.0.0-beta.10",
    "wrangler": "^4.61.0"
  },
  "license": "MIT",
  "packageManager": "bun@1.3.7"
}
